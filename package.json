{
  "name": "chipsy",
  "version": "1.0.0",
  "description": "Chipsy Discord bot and control panel",
  "main": "app.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "node scripts/devRunner.js",
    "dev:bot": "node server/index.js",
    "dev:panel": "npm --prefix web run serve",
    "dev:mysql": "docker compose up mysql",
    "bot": "node app.js",
    "panel:build": "npm --prefix web run build",
    "test": "jest",
    "migrate:exp-columns": "node scripts/migrateExpColumns.js",
    "migrate:bigint-columns": "node scripts/migrateBigIntColumns.js",
    "migrate:log-indexes": "node scripts/migrateLogIndexes.js"
  },
  "author": "Sir Volpe",
  "license": "ISC",
  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "express-mysql-session": "^3.0.3",
    "express-rate-limit": "^8.2.0",
    "express-session": "^1.16.2",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "mysql2": "^3.9.7",
    "node-fetch": "^2.6.7",
    "pokersolver": "^2.1.3",
    "redis": "^5.9.0",
    "sanitize-html": "^2.17.0",
    "winston": "^3.18.3",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
