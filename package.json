{
  "name": "chipsy",
  "version": "1.0.0",
  "description": "Chipsy Discord bot and control panel",
  "main": "bot/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "node scripts/devRunner.mjs",
    "dev:bot": "node server/index.js",
