    "canvas": "^3.2.0",
    "chalk": "^5.6.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "express-mysql-session": "^3.0.3",
    "express-rate-limit": "^8.2.0",
    "express-session": "^1.16.2",
    "helmet": "^8.1.0",
    "imgur": "^2.5.0",
    "mysql2": "^3.9.7",
    "node-fetch": "^2.6.7",
    "pokersolver": "^2.1.3",
    "sanitize-html": "^2.17.0",
    "sharp": "^0.34.4",
